<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid
                android:color="#002860" />
            <size
                android:width="40dp"
                android:height="40dp"/>
            <padding android:bottom="10dp"
                android:top="10dp"
                android:left="10dp"
                android:right="10dp" ></padding>
        </shape>
    </item>
    <item android:drawable="@drawable/ic_camera"></item>
</layer-list>
    <!-- layer list para criar uma imagem usando item ordenados-->
